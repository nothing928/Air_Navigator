        android:layout_height="wrap_content"
        android:paddingLeft="10dp"
        android:textSize="20sp"
        android:text="开启注意"
        android:textColor="@color/black"/>
    <TextView
        android:id="@+id/tv_item_m"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="10dp"
        android:textSize="20sp"
        android:text="开启警告"
        android:textColor="@color/black"/>

</LinearLayout>